carbon:
  bind: "127.0.0.0"
  port: 2003
  rollups:
    - 60s:5356800s
    - 900s:62208000s
  aggregatorDelay: 90
  authorizedTenants:
    - "tenant1"
  allowAll: false
store:
  cluster:
    - "cassandra-1"
    - "cassandra-2"
  port: 9042
  userName: 'username'
  userPassword: 'password'
  keyspace: 'metric'
  maxConnections: 2048
  readTimeout: 10
  connectTimeout: 10
  maxRequests: 128
  batch: true
  batchSize: 500
  pool: 2
index:
  cluster:
    - "es-1"
    - "es-2"
  port: 9300
  index: "disthene"
  cache: true
  expire: 3600
  bulk:
    actions: 10000
    interval: 5
stats:
  interval: 60
  tenant: "test"
  hostname: "disthene-1a"
  log: true
